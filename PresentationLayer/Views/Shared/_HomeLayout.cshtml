@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en">

@await Component.InvokeAsync("_HeadPartial")

<body>

    @await Component.InvokeAsync("_SocialsTopPartial")

    @await Component.InvokeAsync("_BannerPartial")

    @await Component.InvokeAsync("_NavbarPartial")

    <div class="py-30"></div>

    
    @RenderSection("_FeaturePartial",false)
    

    <section class="block-wrapper">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">

                   


                        @RenderBody()
                    

                   
                </div>

                

                

                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                    <div class="sidebar">
                        @await Component.InvokeAsync("_StayInTouchPartial")
                        @await Component.InvokeAsync("_HotNewsPartial")
                        @*@await Component.InvokeAsync("_BeautyBlogPartial")*@
                    </div>
                </div>
            </div>
        </div>
    </section>
   


    
    @RenderSection("TourGameHealth",false)
  
    @RenderSection("scripts", false)

   

    @await Component.InvokeAsync("_FooterPartial")


    @await Component.InvokeAsync("_ScriptPartial")

</body>
</html>


